   "source": [
    "def get_profit(prices):\n",
    "    import numpy as np\n",
    "    prices = np.array(prices)\n",
    "    profit = prices.reshape(-1, 1) - prices\n",
    "    cummin = np.minimum.accumulate\n",
    "    return np.max(profit - cummin(profit))"
   ]
  },
