   "source": [
    "### Tallest spectator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "l = [3, 6, 3, 4, 1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "def spectators(l):\n",
    "    spec = 1\n",
    "    tallest = l[-1]\n",
    "    \n",
    "    for i in range(len(l)-1)[::-1]:\n",
    "        if l[i] > tallest:\n",
    "            spec += 1\n",
    "            tallest = l[i]\n",
    "    return spec"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "3"
      ]
     },
     "execution_count": 28,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "spectators(l)"
   ]
